    <font color="#01549b"><h2>Login</h2></font>
    <form action="/VerUsuario" method="get" >

  <div class="form-group row">
    <label for="email" class="col-1 col-form-label"><h5><b>Email:</b></h5></label>

      <input class="form-control" type="email"
             placeholder="Ingresa tu email"
             name="email"
             id="email"
             required
      >
    </div>

  <div class="form-group row">
    <label for="password" class="col-1 col-form-label"><h5><b>Contraseña:</b></h5></label>

      <input class="form-control" type="password"
             placeholder="Ingresa tu contraseña"
             name="password"
             id="password"
             required
      >

  </div>


      <button  type="submit"
               class="btn btn-outline-success">Entrar</button>
      <a href="/signup">
        <button type="button" class="btn btn-primary">
          Si aun no tienes cuenta regístrate!!
        </button>
      </a>
  </form>

